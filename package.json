  "authors": [
    "Guillaume Grossetie (https://github.com/mogztter)"
  ],
  "description": "blog.yuzutech.fr website",
  "devDependencies": {
    "asciidoctor.js": "1.5.7",
    "browser-sync": "^2.26.3",
    "chokidar": "^2.0.4",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build": "npm run build-clean && npm run build-ui && npm run build-pages && npm run build-images",
    "build-ui": "cd ui && npm run build && cd - && node tasks/ui/copy.js",
    "build-clean": "rimraf public/*",
    "build-pages": "node tasks/pages/build.js",
    "build-images": "node tasks/images/build.js",
    "deploy": "npm run build",
    "start": "node tasks/watch.js"
  }
}
